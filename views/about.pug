script.
    const hideSections = (exception) => {
        const options = ['experienceInfo', 'projectInfo', 'blogInfo', 'toolsInfo']
        const optionsWithoutException = options.map(option => {
            if (option !== exception) {
                document.getElementById(option).setAttribute("class", "info-hidden");
            }
        })
    }

    const unselectItems = (exception) => {
        const options = ['nav-item-experience', 'nav-item-projects', 'nav-item-blog', 'nav-item-tools']
        const optionsWithoutException = options.map(option => {
            if (option !== exception) {
                document.getElementById(option).setAttribute("class", "navItem");
            }
        })
    }

    const onExperienceClick = () => {
       document.getElementById('experienceInfo').setAttribute("class", "info-display");
       document.getElementById('nav-item-experience').setAttribute("class", "navItemSelected");
       hideSections('experienceInfo')
       unselectItems('nav-item-experience')
    };

     const onToolsClick = () => {
       document.getElementById('toolsInfo').setAttribute("class", "info-display");
       document.getElementById('nav-item-tools').setAttribute("class", "navItemSelected");
       hideSections('toolsInfo')
       unselectItems('nav-item-tools')
    };

    const onProjectsClick = () => {
       document.getElementById('projectInfo').setAttribute("class", "info-display");document.getElementById('nav-item-projects').setAttribute("class", "navItemSelected");
       hideSections('projectInfo')
       unselectItems('nav-item-projects')
    };

    const onBlogClick = () => {
       document.getElementById('blogInfo').setAttribute("class", "info-display");
       document.getElementById('nav-item-blog').setAttribute("class", "navItemSelected");
       hideSections('blogInfo')
       unselectItems('nav-item-blog')
    };
